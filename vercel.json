{
    "version": 2,
    "builds": [
        {
            "src": "app/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": { "distDir": "app/build" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/server.js"
        }
    ]
}
